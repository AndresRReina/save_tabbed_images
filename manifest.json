{
  "name": "Save Tabbed Images",
  "description": "Save images currently opened in tabs.",
  "version": "0.1.2",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "downloads",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon16.png"
  },
  "manifest_version": 2,
  "icons": {
  	"16": "icon16.png",
  	"48": "icon48.png",
  	"128": "icon128.png"
  },
  "content_scripts": [{
    "js": [
      "jquery-2.1.1.min.js",
      "popup.js"
    ],
    "matches": [
      "http://*/*",
      "https://*/*"
    ]
  }]
}
